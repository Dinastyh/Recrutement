ipv4 -> (?:(?:25[0-5]|2[0-4][0-9]|[0-1]?[0-9][0-9]?)\.){3}(?:25[0-5]|2[0-4][0-9]|[0-1]?[0-9][0-9]?)

ipv6 -> (:?[0-9a-fA-F]{1,4}:){1,2}(:?:[0-9a-fA-F]{1,4}){1,5}|[0-9a-fA-F]{1,4}:(:?(:?:[0-9a-fA-F]{1,4}){1,6})|:(:?(:?:[0-9a-fA-F]{1,4}){1,7}|:)|fe80:(:?:[0-9a-fA-F]{0,4}){0,4}%[0-9a-zA-Z]{1,}|::(:?ffff(:?:0{1,4}){0,1}:){0,1}(:?(:?25[0-5]|(:?2[0-4]|1{0,1}[0-9]){0,1}[0-9])\.){3,3}(:?25[0-5]|(:?2[0-4]|1{0,1}[0-9]){0,1}[0-9])|(:?[0-9a-fA-F]{1,4}:){1,4}:(:?(:?25[0-5]|(:?2[0-4]|1{0,1}[0-9]){0,1}[0-9])\.){3,3}(:?25[0-5]|(:?2[0-4]|1{0,1}[0-9]){0,1}[0-9]))
domain -> ?
email -> (?:[.-w]+)@(?:(?:[\w-]+\.)+[\w-]{2,4})
date -> (?:Mon|Tue|Wed|Thu|Fri|Sat|Sun), (?:[01][1-9]) (?:Jan|Feb|Mar|Apr|May|Jun|Jul|Aug|Sep|Oct|Nov|Dec)
